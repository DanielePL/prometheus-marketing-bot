{
  "name": "prometheus-server",
  "version": "1.0.0",
  "description": "Prometheus Marketing Bot - Backend API Server",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "seed": "node src/scripts/seed.js",
    "seed:dev": "NODE_ENV=development node src/scripts/seed.js",
    "backup": "node src/scripts/backup.js",
    "restore": "node src/scripts/restore.js",
    "deploy": "pm2 start ecosystem.config.js --env production"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.0",
    "openai": "^4.103.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "express",
    "mongodb",
    "mongoose",
    "api",
    "marketing",
    "ai",
    "prometheus"
  ],
  "author": "Prometheus Team",
  "license": "MIT"
}
